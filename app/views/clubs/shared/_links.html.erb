<%= link_to club_members_path(@club) do %>
    <div class="button-effect-3 members">
        <i class="fas fa-users"></i>
    </div>
<% end %>
<%= link_to @club.website_url, target:"_blank" do %>
    <div class="button-effect-3 website">
        <i class="fas fa-link"></i>
    </div>
<% end %>
<%= link_to @club.instagram_url, target:"_blank" do %>
    <div class="button-effect-3 instagram">
        <i class="fab fa-instagram"></i>
    </div>
<% end %>
<%= link_to @club.linkedin_url, target:"_blank" do %>
    <div class="button-effect-3 linkedin">
        <i class="fab fa-linkedin"></i>
    </div>
<% end %>